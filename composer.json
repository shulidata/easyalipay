{
    "name": "shulidata/easyalipay",
    "description": "支付宝SDK",
    "keywords": [
        "alipay",
        "easyaliapy",
        "alipay-sdk",
        "sdk"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.2",
        "pimple/pimple": "^3.0",
        "symfony/cache": "^3.3 || ^4.0",
        "symfony/http-foundation": "^2.7 || ^3.0 || ^4.0",
        "symfony/psr-http-message-bridge": "^0.3 || ^1.0"
    },
    "autoload": {
        "psr-4": {
            "EasyAlipay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasyAlipay\\Tests\\": "tests/"
        }
    }
}
